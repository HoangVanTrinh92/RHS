<%= render "shared/filter" %>
<h3><%= t "search.result" %></h3>
<div class="panel panel-info">
  <div class="panel-heading">
    <% if @addresses.blank? %>
      <%= t "search.no_data" %>
    <% else %>
      <%= render "searchs/sorting_search" %>
      <%= render @addresses %>
      <%= paginate @addresses %>
    <% end %>
  </div>
</div>
